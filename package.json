{
  "name": "flex",
  "version": "1.0.0",
  "description": "An intelligent candidate-job posting tool",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nramabad/Flex.git"
  },
  "keywords": [
    "MongoDB",
    "Express",
    "React",
    "Node",
    "Job",
    "Resume",
    "Candidate",
    "Hiring",
    "Sentiment",
    "Keyword"
  ],
  "author": "Navaneet Ramabadran, Christina Chu, George Wang, Adi Kantawala",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nramabad/Flex/issues"
  },
  "homepage": "https://github.com/nramabad/Flex#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "keyword-extractor": "0.0.15",
    "mongoose": "^5.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.2",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "retext-keywords": "^4.0.2",
    "string-similarity": "^3.0.0",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
